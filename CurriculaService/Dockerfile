FROM openjdk:8

EXPOSE 1910

ARG DB_NAME

ENV DB_NAME=${DB_NAME}

ARG DB_PASSWORD

ENV DB_PASSWORD=${DB_PASSWORD}

ARG DB_PORT

ENV DB_PORT=${DB_PORT}

ARG DB_SCHEMA

ENV DB_SCHEMA=${DB_SCHEMA}

ARG DB_URL

ENV DB_URL=${DB_URL}

ARG DB_USERNAME

ENV DB_USERNAME=${DB_USERNAME}

ARG DB_NAME

ENV DB_NAME=${DB_NAME}

ARG DB_DEPLOYMENT_STAGE

ENV DB_DEPLOYMENT_STAGE=${DB_DEPLOYMENT_STAGE}

COPY target/CurriculaService-0.0.1-SNAPSHOT.jar CurriculaService.jar

CMD ["java", "-jar", "CurriculaService.jar"]